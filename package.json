{
  "name": "alice-asset-manager",
  "version": "1.1.0",
  "description": "Node.js API for managing images and sounds in Alice skill",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/alice-asset-manager.git"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "src/index.js",
  "files": [
    "src/",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src test",
    "test": "mocha test/setup.js test/specs/*.js --timeout=6000",
    "test:d": "DEBUG=alice-asset-manager npm run test -- --bail",
    "toc": "markdown-toc README.md -i",
    "release": "npm run lint && npm test && npm version $VER && npm publish && git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": "eslint"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "form-data": "^3.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "micro": "^9.3.4",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.0",
    "throw-utils": "^0.1.3"
  },
  "devDependencies": {
    "alice-tester": "^0.9.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-mocha": "^6.2.1",
    "get-port": "^5.0.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "markdown-toc": "^1.2.0",
    "mocha": "^6.2.2",
    "p-wait-for": "^3.1.0",
    "sinon": "^7.5.0"
  },
  "license": "MIT"
}
