{
  "name": "alice-asset-manager",
  "version": "1.0.0",
  "description": "A library for managing images and sounds in Alice skill",
  "author": {
    "name": "Vitaliy Potapov",
    "email": "noginsk@rambler.ru"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vitalets/alice-asset-manager.git"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src test",
    "test": "mocha test/setup.js test/specs/*.js --timeout=5000",
    "toc": "markdown-toc README.md -i",
    "prerelease": "npm run lint && npm test",
    "release": "npm version $VER && npm publish",
    "postrelease": "git push --follow-tags --no-verify",
    "release-patch": "VER=patch npm run release",
    "release-minor": "VER=minor npm run release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": "eslint"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0",
    "throw-utils": "^0.1.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-mocha": "^6.2.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "markdown-toc": "^1.2.0",
    "mocha": "^6.2.2",
    "p-wait-for": "^3.1.0"
  },
  "license": "MIT"
}
